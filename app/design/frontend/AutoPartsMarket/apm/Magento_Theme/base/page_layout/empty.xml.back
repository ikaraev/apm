<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_layout.xsd">
    <referenceContainer name="page.wrapper">
        <container name="intermediate.content"
                   as="intermediate_content"
                   htmlTag="div"
                   htmlId="intermediate-content"
                   htmlClass="intermediate-content"
                   before="main.content">
        </container>
    </referenceContainer>
</layout>
